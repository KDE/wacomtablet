msgid "Absolute"
msgstr "Absolutua"

#: src/applet/package/contents/ui/FullRepresentation.qml:274
#, kde-format
msgctxt "relative pen movement (mouse mode)"
msgid "Relative"
msgstr "Erlatiboa"

