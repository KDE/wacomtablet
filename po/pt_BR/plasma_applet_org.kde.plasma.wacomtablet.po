#, kde-format
msgctxt "absolute pen movement (pen mode)"
msgid "Absolute"
msgstr "Absoluto"

#: src/applet/package/contents/ui/FullRepresentation.qml:274
#, kde-format
msgctxt "relative pen movement (mouse mode)"
msgid "Relative"
msgstr "Relativo"

