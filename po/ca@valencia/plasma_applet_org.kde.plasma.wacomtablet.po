msgid "Settings"
msgstr "Configuració"

#: src/applet/package/contents/ui/FullRepresentation.qml:218
#, kde-format
msgctxt "Toggle between touch on/off"
msgid "Touch:"
msgstr "Tàctil:"

#: src/applet/package/contents/ui/FullRepresentation.qml:228
