#: src/applet/package/contents/ui/FullRepresentation.qml:36
#, kde-format
msgid "Graphic Tablet - Device not detected."
msgstr "Tableta gráfica - Non se detectou ningún dispositivo."

#: src/applet/package/contents/ui/FullRepresentation.qml:45
#, kde-format
msgid "Error - Tablet service not available."
msgstr "Erro - O servizo de tabletas non está dispoñíbel."

#: src/applet/package/contents/ui/FullRepresentation.qml:106
#, kde-format
msgid ""
"This widget is inactive because your tablet device is not connected or "
"currently not supported."
msgstr ""
"Este trebello está inactivo porque non hai ningún dispositivo de tableta "
"conectado, ou os dispositivos conectados non son compatíbeis."

#: src/applet/package/contents/ui/FullRepresentation.qml:107
#, kde-format
msgid ""
"Please start the KDE wacom tablet service.\n"
"The service is required for tablet detection and profile support."
msgstr ""
"Inicie o servizo de KDE para tabletas de Wacom.\n"
"O servizo é necesario para as funcionalidades de detección de tabletas e uso "
"de perfís."

#: src/applet/package/contents/ui/FullRepresentation.qml:127
#, kde-format
msgid "Select Tablet:"
msgstr "Escolla unha tableta:"

#: src/applet/package/contents/ui/FullRepresentation.qml:155
#, kde-format
msgid "Select Profile:"
msgstr "Escolla o perfil:"

#: src/applet/package/contents/ui/FullRepresentation.qml:211
#, kde-format
